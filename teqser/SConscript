Import('env')
teqser_sources = Split("""
	teqser.cpp""")

# Set any platform specific options for this target
env.UseTargetOptions('teqser')

teqser = env.Program('teqser', teqser_sources, LIBS=['argparse', 'aqsis'], LIBPATH=['$LIBDIR', '$BINDIR'])
env.Install('$BINDIR', teqser)

env.Distribute('teqser', teqser_sources)
env.Distribute('teqser', "SConscript")
env.Distribute('teqser', "Makefile.am")

